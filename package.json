{
  "name": "@defai.digital/ax-cli",
  "version": "4.4.2",
  "sdkVersion": "1.4.0",
  "description": "Enterprise-Class AI Command Line Interface - Primary support for GLM (General Language Model) with multi-provider AI orchestration powered by AutomatosX.",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sdk": {
      "import": "./dist/sdk/index.js",
      "types": "./dist/sdk/index.d.ts"
    },
    "./sdk/types": {
      "import": "./dist/sdk/types.js",
      "types": "./dist/sdk/types.d.ts"
    }
  },
  "bin": {
    "ax-cli": "bin/ax-cli"
  },
  "files": [
    "dist",
    "bin",
    "config-defaults",
    ".ax-cli",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:schemas": "pnpm --filter @defai.digital/ax-schemas build",
    "build:core": "pnpm --filter @defai.digital/ax-core build",
    "build:glm": "pnpm --filter @defai.digital/ax-glm build",
    "build:grok": "pnpm --filter @defai.digital/ax-grok build",
    "build:legacy": "npm run build:schemas && tsc && chmod +x dist/index.js",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "pretest": "npm run build:schemas",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "pnpm -r typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build:legacy",
    "prepare": "husky"
  },
  "keywords": [
    "cli",
    "ai",
    "llm",
    "glm",
    "agent",
    "text-editor",
    "automatosx",
    "orchestration",
    "chatbot",
    "assistant",
    "openai",
    "anthropic",
    "multi-provider",
    "mcp",
    "model-context-protocol",
    "sdk",
    "library",
    "programmatic-api"
  ],
  "author": "DEFAI Digital <https://defai.digital>",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@defai.digital/ax-core": "workspace:*",
    "@defai.digital/ax-schemas": "^1.1.0",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@types/ws": "^8.18.1",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "clipboardy": "^5.0.1",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.2.0",
    "glob": "^13.0.0",
    "ink": "^6.5.1",
    "ink-spinner": "^5.0.0",
    "js-yaml": "^4.1.1",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "node-cache": "^5.1.2",
    "openai": "^6.10.0",
    "react": "^19.2.1",
    "ripgrep-node": "^1.0.0",
    "simple-git": "^3.30.0",
    "tiktoken": "^1.0.21",
    "ts-morph": "^27.0.2",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/defai-digital/ax-cli.git"
  },
  "bugs": {
    "url": "https://github.com/defai-digital/ax-cli/issues"
  },
  "homepage": "https://github.com/defai-digital/ax-cli#readme",
  "preferGlobal": true,
  "lint-staged": {
    "packages/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
