{
  "name": "@defai.digital/ax-cli",
  "version": "3.15.21",
  "sdkVersion": "1.3.0",
  "description": "Enterprise-Class AI Command Line Interface - Primary support for GLM (General Language Model) with multi-provider AI orchestration powered by AutomatosX.",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sdk": {
      "import": "./dist/sdk/index.js",
      "types": "./dist/sdk/index.d.ts"
    },
    "./sdk/types": {
      "import": "./dist/sdk/types.js",
      "types": "./dist/sdk/types.d.ts"
    }
  },
  "bin": {
    "ax-cli": "dist/index.js"
  },
  "files": [
    "dist",
    "config-defaults",
    "packages/schemas/dist",
    "packages/schemas/package.json",
    ".ax-cli",
    "LICENSE",
    "README.md",
    "ax.config.json",
    "eslint.config.js",
    "vitest.config.ts"
  ],
  "scripts": {
    "build": "npm run build:schemas && tsc && chmod +x dist/index.js",
    "build:schemas": "cd packages/schemas && npm run build",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "pretest": "npm run build:schemas",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && node scripts/prepare-publish.js",
    "postpublish": "node scripts/restore-symlink.js"
  },
  "keywords": [
    "cli",
    "ai",
    "llm",
    "glm",
    "agent",
    "text-editor",
    "automatosx",
    "orchestration",
    "chatbot",
    "assistant",
    "openai",
    "anthropic",
    "multi-provider",
    "mcp",
    "model-context-protocol",
    "sdk",
    "library",
    "programmatic-api"
  ],
  "author": "DEFAI Digital <https://defai.digital>",
  "license": "MIT",
  "bundleDependencies": [
    "@ax-cli/schemas"
  ],
  "dependencies": {
    "@ax-cli/schemas": "workspace:*",
    "@clack/prompts": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "clipboardy": "^5.0.1",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.2.0",
    "glob": "^13.0.0",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "js-yaml": "^4.1.1",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "node-cache": "^5.1.2",
    "openai": "^5.10.1",
    "react": "^18.3.1",
    "ripgrep-node": "^1.0.0",
    "simple-git": "^3.30.0",
    "tiktoken": "^1.0.21",
    "ts-morph": "^27.0.2",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/ui": "^4.0.10",
    "eslint": "^9.31.0",
    "tsx": "^4.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/defai-digital/ax-cli.git"
  },
  "bugs": {
    "url": "https://github.com/defai-digital/ax-cli/issues"
  },
  "homepage": "https://github.com/defai-digital/ax-cli#readme",
  "preferGlobal": true
}
