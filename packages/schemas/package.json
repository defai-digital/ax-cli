{
  "name": "@defai.digital/ax-schemas",
  "version": "5.1.8",
  "description": "Zod schema definitions and TypeScript types for AX CLI ecosystem - Shared validation schemas for config, API, tools, and MCP protocols.",
  "repository": {
    "type": "git",
    "url": "https://github.com/defai-digital/ax-cli.git",
    "directory": "packages/schemas"
  },
  "homepage": "https://github.com/defai-digital/ax-cli#readme",
  "bugs": {
    "url": "https://github.com/defai-digital/ax-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "antigravity",
    "sdk",
    "mcp",
    "ide",
    "coding",
    "developer-tools",
    "code-generation",
    "glm",
    "grok",
    "xai",
    "z-ai",
    "qwen",
    "deepseek",
    "ai-coding",
    "vibe-coding",
    "automatosx"
  ],
  "author": "DEFAI Digital <https://defai.digital>",
  "license": "MIT",
  "dependencies": {
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
