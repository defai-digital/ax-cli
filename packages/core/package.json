{
  "name": "@defai.digital/ax-core",
  "version": "1.0.0",
  "description": "Core functionality for ax-cli, ax-glm, and ax-grok - includes all tools, UI, MCP, and agent infrastructure",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/ax-cli.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/anthropics/ax-cli#readme",
  "bugs": {
    "url": "https://github.com/anthropics/ax-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/exports.d.ts",
      "import": "./dist/exports.js"
    },
    "./cli": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./provider": {
      "types": "./dist/provider/index.d.ts",
      "import": "./dist/provider/index.js"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./commands": {
      "types": "./dist/commands/index.d.ts",
      "import": "./dist/commands/index.js"
    },
    "./llm": {
      "types": "./dist/llm/index.d.ts",
      "import": "./dist/llm/index.js"
    },
    "./memory": {
      "types": "./dist/memory/index.d.ts",
      "import": "./dist/memory/index.js"
    },
    "./checkpoint": {
      "types": "./dist/checkpoint/index.d.ts",
      "import": "./dist/checkpoint/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk/index.d.ts",
      "import": "./dist/sdk/index.js"
    }
  },
  "files": [
    "dist",
    "config-defaults"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@defai.digital/ax-schemas": "^1.0.0",
    "@clack/prompts": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "clipboardy": "^5.0.1",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.2.0",
    "glob": "^13.0.0",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "js-yaml": "^4.1.1",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "node-cache": "^5.1.2",
    "openai": "^5.10.1",
    "react": "^18.3.1",
    "ripgrep-node": "^1.0.0",
    "simple-git": "^3.30.0",
    "tiktoken": "^1.0.21",
    "ts-morph": "^27.0.2",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT"
}
