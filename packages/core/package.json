{
  "name": "@defai.digital/ax-core",
  "version": "5.1.11",
  "description": "Core library for AX CLI ecosystem - Shared tools, React UI components, MCP client, LLM agent infrastructure, and AutomatosX integration for ax-cli, ax-glm, and ax-grok.",
  "repository": {
    "type": "git",
    "url": "https://github.com/defai-digital/ax-cli.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/defai-digital/ax-cli#readme",
  "bugs": {
    "url": "https://github.com/defai-digital/ax-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/exports.d.ts",
      "import": "./dist/exports.js"
    },
    "./cli": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./provider": {
      "types": "./dist/provider/index.d.ts",
      "import": "./dist/provider/index.js"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./commands": {
      "types": "./dist/commands/index.d.ts",
      "import": "./dist/commands/index.js"
    },
    "./llm": {
      "types": "./dist/llm/index.d.ts",
      "import": "./dist/llm/index.js"
    },
    "./memory": {
      "types": "./dist/memory/index.d.ts",
      "import": "./dist/memory/index.js"
    },
    "./checkpoint": {
      "types": "./dist/checkpoint/index.d.ts",
      "import": "./dist/checkpoint/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk/index.d.ts",
      "import": "./dist/sdk/index.js"
    },
    "./design-check": {
      "types": "./dist/design-check/index.d.ts",
      "import": "./dist/design-check/index.js"
    }
  },
  "files": [
    "dist",
    "config-defaults",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b && cp -r src/i18n/locales dist/i18n/ && cp -r config-defaults dist/",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@defai.digital/ax-schemas": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@types/ws": "^8.18.1",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "clipboardy": "^5.0.1",
    "colord": "^2.9.3",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "ink": "^6.5.1",
    "ink-spinner": "^5.0.0",
    "js-yaml": "^4.1.1",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "node-cache": "^5.1.2",
    "openai": "^6.10.0",
    "react": "^19.2.1",
    "ripgrep-node": "^1.0.0",
    "simple-git": "^3.30.0",
    "tiktoken": "^1.0.22",
    "ts-morph": "^27.0.2",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "keywords": [
    "antigravity",
    "sdk",
    "mcp",
    "ide",
    "coding",
    "developer-tools",
    "code-generation",
    "glm",
    "grok",
    "xai",
    "z-ai",
    "qwen",
    "deepseek",
    "ai-coding",
    "vibe-coding",
    "automatosx"
  ]
}
