{
  "name": "@defai.digital/ax-grok",
  "version": "5.1.10",
  "description": "Grok AI Coding Assistant CLI powered by xAI - Terminal-based vibe coding with Grok 4 thinking mode, vision, live web search, and MCP integration. Part of the AX CLI ecosystem.",
  "repository": {
    "type": "git",
    "url": "https://github.com/defai-digital/ax-cli.git",
    "directory": "packages/ax-grok"
  },
  "homepage": "https://github.com/defai-digital/ax-cli#readme",
  "bugs": {
    "url": "https://github.com/defai-digital/ax-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ax-grok": "./bin/ax-grok"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk.d.ts",
      "import": "./dist/sdk.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@defai.digital/ax-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "keywords": [
    "antigravity",
    "sdk",
    "mcp",
    "ide",
    "coding",
    "developer-tools",
    "code-generation",
    "glm",
    "grok",
    "xai",
    "z-ai",
    "qwen",
    "deepseek",
    "ai-coding",
    "vibe-coding",
    "automatosx"
  ]
}
